<div class="test_relative">
  <div class="test_absolute">
<h1>Welcome to Weekend Planner </h1>

<%= form_tag(login_path) do %>
  <p><%= label_tag :email %>
  <%= email_field_tag :email %></p>

  <p><%= label_tag :password %>
  <%= password_field_tag :password %></p>

  <%= submit_tag "Log in" %>
<% end %>

<%= button_to 'Sign Up', new_user_path, method: :get  %>
</div>

<div class="homepage-hero-module">
    <div class="video-container">
        <div class="filter"></div>
        <video autoplay loop class="fillWidth">
          <source src="/assets/Sunset-Lapse.mp4" type="video/mp4"
        </video>

    </div>
</div>
</div>
